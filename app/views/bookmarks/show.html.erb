<table class="table table-striped table-hover">
      <tr>
        <th>Image</th>
        <th>Product</th>
        <th>Walmart Price</th>
        <th>Amazon Price</th>
        <th>Actions</th>
      </tr>
      
      <tr>
        <td><img src=<%=@bookmark.image%> width=200px></td>
        <td><%= @bookmark.name %></td>
        <td>
        <%=number_to_currency(@bookmark.walmart_price)%>   <a href=<%=%> target="_blank"><span class="glyphicon glyphicon-eye-open"></span></a>
        <p><a type="button" class="btn btn-primary btn-sm" href="<%=@bookmark.add_to_cart%>" target="_blank">
               <span class="glyphicon glyphicon-shopping-cart"></span> Add to Cart
        </a></p>
        </td>
        <td>
        <%=number_to_currency(@bookmark.amazon_price)%>   <a href=<%=%> target="_blank"><span class="glyphicon glyphicon-eye-open"></span></a>
        </td>
        <td>
          <a href="/delete_bookmark/<%= @bookmark.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
    </table>