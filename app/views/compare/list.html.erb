<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
  <tr>
    <th>Image</th>
    <th>Product</th>
    <th>Discount</th>    
    <th>Walmart Price</th>
    <th>Amazon Price</th>
    <th>UPC</th>
  </tr>
  <%@array3.each do |entry|%>
  <tr>
      <td><img src=<%=entry[:image]%> width=200px></td>
      <td><%=entry[:title]%></td>
      <td><%=entry[:discount]%>%</td>      
      <td><%=number_to_currency(entry[:walmart_price])%></td>
      <td><%=number_to_currency(entry[:amazon_price])%></td>
      <td><%=entry[:upc]%></td>
  </tr>
<%end%>
</table>
</div>
</div>

<form action = "/list">
<input type="submit" value="Click for more">
</form>
